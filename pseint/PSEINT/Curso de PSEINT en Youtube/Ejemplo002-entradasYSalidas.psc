Algoritmo Ejemplo002
	Definir num Como Entero;
	num  = 0;
	Escribir "Dime el valor de un número entero, por favor: ";
	Leer num;
	Escribir "El valor del número es: ", num;
	Escribir "Y si lo multiplicamos por 2 es: " Sin Saltar;
	Escribir num * 2;
	Escribir "Esta frase debe aparecer en una nueva línea.";	
